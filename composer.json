{
    "name": "laracraft-tech/laravel-dynamic-model",
    "description": "Dynamic Model for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Zacharias Creutznacher",
            "email": "zacharias@laracraft.tech",
            "homepage": "https://laracraft.tech"
        }
    ],
    "homepage": "https://github.com/laracraft-tech/laravel-dynamic-model",
    "keywords": ["laravel", "eloquent", "model", "dynamic model", "dynamic table", "dynamic schema"],
    "require": {
        "php": ">=7.4",
        "laravel/framework": "^5.3|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "LaracraftTech\\LaravelDynamicModel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaracraftTech\\LaravelDynamicModel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "LaracraftTech\\LaravelDynamicModel\\DynamicModelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
